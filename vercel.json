{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/views/(.*)",
      "dest": "/public/views/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ]
}